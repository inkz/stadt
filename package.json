{
  "name": "stadt-workspace",
  "private": true,
  "devDependencies": {
    "husky": "^1.3.1",
    "lerna": "^3.16.4",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
