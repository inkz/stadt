{
  "name": "stadt",
  "version": "0.1.0",
  "description": "An ADT for TypeScript types.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.4",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.0.3",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "typescript": "^3.5.1"
  },
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha --extension \"ts\" --recursive -r ts-node/register tests/*.ts",
    "prepare": "npm run-script build"
  },
  "author": "r2c",
  "license": "MIT",
  "repository": "https://github.com/returntocorp/stadt",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
